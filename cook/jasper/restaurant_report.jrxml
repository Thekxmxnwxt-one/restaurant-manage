    <query language="sql"><![CDATA[SELECT o.id AS order_id,
	o.ordered_at,
	c.name AS customer_name,
	c.phone AS customer_phone,
	t.table_number,
	e.name AS employee_name,
	i.unit_price,
	i.quantity,
	m.name AS menu_name
FROM orders o
	INNER JOIN customers c ON
	 o.customer_id = c.id
	INNER JOIN tables t ON
	 o.table_id = t.id
	INNER JOIN employees e ON
	 o.employee_id = e.id
	INNER JOIN order_items i ON
	 i.order_id = o.id
	INNER JOIN menu_items m ON
	 i.menu_item_id = m.id
WHERE
	 o.id = 1
ORDER BY m.name DESC]]></query>
